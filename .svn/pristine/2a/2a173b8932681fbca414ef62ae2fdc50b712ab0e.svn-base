      <a href="#announcement" class="btn btn-info" data-toggle="collapse">Announcements</a>
  <div id="announcement" class="collapse">
  &nbsp;
  
  
							<div class="table-wrapper">
								<table class="alt">
									<thead>
										<tr>
											<th>Date</th>
											<th>Description</th>
										</tr>
									</thead>
									<tbody>
									{{#each announcements}}
										<tr>
											<td>{{date}}</td>
											<td>{{{announcement_content}}}</td>
										</tr>
										{{/each}}
									</tbody>
									<tfoot>
										<tr>
											<td colspan="2"></td>
										</tr>
									</tfoot>
								</table>
								
							</div>
							
							
						</section>
						
							<div class="centercol"> 
							&ensp;
							<h2> View all professors </h2>
							
							
							
  <p>View all the registered professors.</p>
  &ensp;
  
  
  
  
  
 
							<div class="table-wrapper">
							&nbsp;&nbsp;
								<table id = "indextable" class="indextable alt">
									<thead>
										<tr>
											<th></th>
											
											
											<th class=" shrink text-left"><a href="javascript:SortTable(1,'T');" class="btn btn-default">Name</button></a></th>
										<th class="shrink text-left"><a href="javascript:SortTable(2,'T');" class="btn btn-default" >Email</button></a></th>
<th class="shrink text-left"><a href="javascript:SortTable(3,'T');" class="btn btn-default" >Role</button></a></th>
<th class="text-left"><a href="javascript:SortTable(4,'T');" class="btn btn-default" >Major</button></a></th>
<th class="text-left"><a href="javascript:SortTable(5,'T');" class="btn btn-default" >Year</button></a></th>
<th class="text-left"><a  class="btn btn-default" >Email <br> Confirmed</button></a></th>
<th class="text-left"><a class="btn btn-default" >Account<br>Status</button></a></th>
											
										</tr>
									</thead>
									<tbody>
									{{#each users}}
										<tr>
											
											
											<td class="shrink" align="center"><a href="/users/{{id}}/edit"  class="btn btn-info">Edit<span class="glyphicon glyphicon-pencil"></span></a></td>
											<td class="shrink">{{last_name}} , {{first_name}}</td>
											<td class="shrink"><a href= "mailto:{{email}}?Subject=Rate%20and %20Review"><span class="glyphicon glyphicon-envelope"></span> {{email}}</a></td>
											<td class="shrink">{{role_string}}</td>
											<td class="shrink">{{major}}</td>
											<td class="shrink">{{year_string}}</td>
											<td class="shrink" align="center"><span class="{{confirmed_icon}}"</span></td>
											<td class="shrink" align="center"><span class="{{active_icon}}"</span></td>
											
																			
										
											
										</tr>
									{{/each}}
									</tbody>
									<tfoot>
									</tfoot>
								</table>
								
							</div>
							 
						</section>
						
					
  
 

